<?xml version="1.0" encoding="utf-8"?>
<!--suppress NavigationFile -->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            app:startDestination="@id/tasksFragment">

    <fragment
            android:id="@+id/profileFragment"
            android:name="com.ilyap.yutaold.ui.fragments.ProfileFragment"
            android:label="fragment_profile"
            tools:layout="@layout/fragment_profile">
    </fragment>
    <fragment
            android:id="@+id/readonlyProfileFragment"
            android:name="com.ilyap.yutaold.ui.fragments.ReadonlyProfileFragment"
            android:label="ReadonlyProfileFragment">
        <argument
                android:name="userId"
                app:argType="integer"
                android:defaultValue="-1" />
    </fragment>
    <fragment
            android:id="@+id/projectsFragment"
            android:name="com.ilyap.yutaold.ui.fragments.ProjectsFragment"
            android:label="fragment_projects"
            tools:layout="@layout/fragment_projects">
        <action
                android:id="@+id/action_projectsFragment_to_readonlyProfileFragment"
                app:destination="@id/readonlyProfileFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
            android:id="@+id/tasksFragment"
            android:name="com.ilyap.yutaold.ui.fragments.TasksFragment"
            android:label="fragment_tasks"
            tools:layout="@layout/fragment_tasks" />
    <fragment
            android:id="@+id/teamsFragment"
            android:name="com.ilyap.yutaold.ui.fragments.TeamsFragment"
            android:label="fragment_teams"
            tools:layout="@layout/fragment_teams">
        <action
                android:id="@+id/action_teamsFragment_to_readonlyProfileFragment"
                app:destination="@id/readonlyProfileFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
            android:id="@+id/searchFragment"
            android:name="com.ilyap.yutaold.ui.fragments.SearchFragment"
            android:label="fragment_search"
            tools:layout="@layout/fragment_search">
        <action
                android:id="@+id/action_searchFragment_to_readonlyProfileFragment"
                app:destination="@id/readonlyProfileFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>